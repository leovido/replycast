# Enable mocks for local development (set to 'true' to use mock data)
NEXT_PUBLIC_USE_MOCKS=true

# Mock network delay in milliseconds (default: 500ms)
NEXT_PUBLIC_MOCK_DELAY=500

# Quotient API key (only needed when mocks are disabled)
QUOTIENT_API_KEY=your_quotient_api_key_here

# Neynar API key (only needed when mocks are disabled)
NEYNAR_API_KEY=your_neynar_api_key_here
